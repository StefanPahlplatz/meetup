{
  "extends": "airbnb",
  "globals": {
    "fetch": false
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-underscore-dangle": "warn",
    "global-require":"warn",
    "import/prefer-default-export": "off"
  },
  "parser": "babel-eslint"
}
